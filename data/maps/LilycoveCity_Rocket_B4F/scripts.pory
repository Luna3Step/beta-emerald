script LilycoveCity_Rocket_B4F_PP_MAX {
    lockall
    msgbox(format("You found a PP Max!"))
    additem(ITEM_PP_MAX, 1)
    if(var(VAR_RESULT) == TRUE){
		removeobject(8)
		setflag(FLAG_B4F_PPMAX)
		waitmessage
		releaseall
		end
	}
	else{
        call(Common_EventScript_ShowBagIsFull)
        releaseall
	}
}