script LilycoveCity_Rocket_B3F_Elixir {
    lockall
    msgbox(format("You found an Elixir!"))
    additem(ITEM_ELIXIR, 1)
    if(var(VAR_RESULT) == TRUE){
		removeobject(7)
		setflag(FLAG_B3F_ELIXIR)
		waitmessage
		releaseall
		end
	}
	else{
        call(Common_EventScript_ShowBagIsFull)
        releaseall
	}
}

